   "source": [
    "N = 100\n",
    "table = [True] * N # 無罪推定, 先假定所有數字都是質數\n",
    "table[0] = table[1] = False # 0 和 1 不是質數\n",
    "for i in range(2,N):\n",
    "    if table[i]:\n",
    "        # 所有 i 的倍數都不是質數\n",
    "        for j in range(i*i, N, i):\n",
    "            table[j] = False"
   ]
